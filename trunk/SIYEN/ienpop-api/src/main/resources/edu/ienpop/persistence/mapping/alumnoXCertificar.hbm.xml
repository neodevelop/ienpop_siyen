<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="edu.ienpop.model.AlumnoXCertificar" table="alumno_x_certificar">
		<id name="id" column="idAlumnoXCertificar">
			<generator class="increment"/>
		</id>
		<property name="nombreCompleto" insert="false"/>
		<property name="idCursoXCertificar" insert="false"/>
		<property name="idStatusAlumno" insert="false"/>
		<property name="fechaHoraRegistro" insert="false"/>
		<property name="observaciones" insert="false"/>
	</class>
</hibernate-mapping>